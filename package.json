{
  "name": "nettingjs",
  "version": "1.0.0",
  "type": "module",
  "description": "A neural network based web tracker library.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npx rollup -c",
    "test": "jest",
    "dist": "npm run build",
    "clean": "del ./dist",
    "prepare": "husky install",
    "watch:node": "rollup -c rollup.config.js -w",
    "lint": "eslint --fix --ext .js,.ts src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rezwanahmodsami/nettingjs.git"
  },
  "keywords": [
    "nettingjs",
    "netting.js",
    "netting"
  ],
  "author": "Rezwan ahmed sami",
  "license": "MIT",
  "browser": "dist/netting.min.js",
  "bugs": {
    "url": "https://github.com/rezwanahmodsami/nettingjs/issues"
  },
  "homepage": "https://github.com/rezwanahmodsami/nettingjs#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "engines": {
    "node": ">=16.0.x"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/jest": "^29.5.1",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "rollup": "^3.21.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "**/*": "npx prettier --write --ignore-unknown"
  },
  "unpkg": "dist/netting.min.js"
}
